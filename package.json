		"lint": "vue-cli-service lint",
		"test": "jest"
	},
	"dependencies": {
		"@auth0/auth0-spa-js": "^1.7.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.27",
		"@fortawesome/free-solid-svg-icons": "^5.12.1",
		"@fortawesome/vue-fontawesome": "^0.1.9",
		"core-js": "^3.6.4",
		"firebase": "^7.14.1",
		"jest-config": "latest",
		"register-service-worker": "^1.6.2",
		"vue": "^2.6.11",
		"vue-router": "^3.1.6",
		"vuex": "^3.1.3",
		"vuexfire": "^3.2.2"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.2.0",
		"@vue/cli-plugin-eslint": "~4.2.0",
		"@vue/cli-plugin-pwa": "^4.2.3",
		"@vue/cli-service": "~4.2.0",
		"@vue/test-utils": "^1.0.0-beta.32",
		"axios": "^0.19.2",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^21.2.0",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.1.2",
		"handlebars": "^4.7.6",
		"jest": "^21.2.1",
		"node-sass": "^4.13.1",
		"sass-loader": "^8.0.2",
		"vue-jest": "^3.0.5",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true,
			"jest": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"transform": {
			".*\\.(vue)$": "vue-jest",
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest"
		},
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.{js,vue}",
			"!**/node_modules/**",
			"!**/dist/**",
			"!**/coverage/**"
		],
		"coverageReporters": [
			"html",
			"text-summary"
		]
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
